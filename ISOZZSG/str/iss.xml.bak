<uta:record uta:id="iss"
            uta:dim="dimIss"
            uta:comment="Issuer Structure"
            xmlns:uta="http://www.ingenico.com/xml/uta">
    <uta:field uta:id="issID"
               uta:type="byte"
               uta:comment='Issuer ID' />
    <uta:field uta:id="issRef"
               uta:type="ASCII"
               uta:length="lenIssRef"
               uta:comment='Issuer Referral Number' />
    <uta:field uta:id="issOpt1"
               uta:type="byte"
               uta:comment='Issuer Options 1' />
    <uta:field uta:id="issOpt2"
               uta:type="byte"
               uta:comment='Issuer Options 2' />
    <uta:field uta:id="issOpt3"
               uta:type="byte"
               uta:comment='Issuer Options 3' />
    <uta:field uta:id="issOpt4"
               uta:type="byte"
               uta:comment='Issuer Options 4' />
    <uta:field uta:id="issDefAcct"
               uta:type="byte"
               uta:comment='Issuer Default Account' />
    <uta:field uta:id="issFloorLimit"
               uta:type="word"
               uta:comment="Floor Limit" />
    <uta:field uta:id="issOpt5"
               uta:type="byte"
               uta:comment='Issuer Options 5' />
    <uta:field uta:id="issPinOpt"
               uta:type="byte"
               uta:comment="Pin option" />
    <uta:data>
        <uta:instance uta:id="issIssuer1">
            <uta:col uta:data='1' />				<!-- issID: Issuer ID (Saved in structure but not used in credit template) -->
            <uta:col uta:data='"7519206"' />		<!-- issRef: Issuer Referral Number -->
            <uta:col uta:data='0x9E' />				<!-- issOpt1: Issuer Options 1-->
            <uta:col uta:data='0xDC' />				<!-- issOpt2: Issuer Options 2-->
            <uta:col uta:data='0xD1' />				<!-- issOpt3: Issuer Options 3-->
            <uta:col uta:data='0x01' />				<!-- issOpt4: Issuer Options 4-->
            <uta:col uta:data='0' />				<!-- issDefAcct: Issuer Default Account-->
            <uta:col uta:data='0' />				<!-- issFloorLimit: Floor Limit-->
            <uta:col uta:data='0xF0' />				<!-- issOpt5: Issuer Options 5-->
            <uta:col uta:data='0x30' />				<!-- issPinOpt: Pin Option-->
        </uta:instance>
        
        <uta:instance uta:id="issIssuer2">
            <uta:col uta:data='2' />				<!-- issID: Issuer ID (Saved in structure but not used in credit template) -->
            <uta:col uta:data='"7519206"' />		<!-- issRef: Issuer Referral Number -->
            <uta:col uta:data='0x9E' />				<!-- issOpt1: Issuer Options 1-->
            <uta:col uta:data='0xDC' />				<!-- issOpt2: Issuer Options 2-->
            <uta:col uta:data='0xD1' />				<!-- issOpt3: Issuer Options 3-->
            <uta:col uta:data='0x01' />				<!-- issOpt4: Issuer Options 4-->
            <uta:col uta:data='0' />				<!-- issDefAcct: Issuer Default Account-->
            <uta:col uta:data='0' />				<!-- issFloorLimit: Floor Limit-->
            <uta:col uta:data='0xF0' />				<!-- issOpt5: Issuer Options 5-->
            <uta:col uta:data='0x31' />				<!-- issPinOpt: Pin Option-->
        </uta:instance> 
        
        <uta:instance uta:id="issIssuer3">
            <uta:col uta:data='3' />				<!-- issID: Issuer ID (Saved in structure but not used in credit template) -->
            <uta:col uta:data='"1000"' />		<!-- issRef: Issuer Referral Number -->
            <uta:col uta:data='0x9E' />				<!-- issOpt1: Issuer Options 1-->
            <uta:col uta:data='0xDC' />				<!-- issOpt2: Issuer Options 2-->
            <uta:col uta:data='0xD1' />				<!-- issOpt3: Issuer Options 3-->
            <uta:col uta:data='0x01' />				<!-- issOpt4: Issuer Options 4-->
            <uta:col uta:data='0' />				<!-- issDefAcct: Issuer Default Account-->
            <uta:col uta:data='0' />				<!-- issFloorLimit: Floor Limit-->
            <uta:col uta:data='0xF0' />				<!-- issOpt5: Issuer Options 5-->
            <uta:col uta:data='0x30' />				<!-- issPinOpt: Pin Option-->
        </uta:instance>               
    </uta:data>
</uta:record>
