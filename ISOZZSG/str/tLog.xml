<uta:record uta:id='tLog'
            uta:dim='dimTLog'
            uta:cache="no"
            uta:comment='BRI Transaction log table'
            xmlns:uta="http://www.ingenico.com/xml/uta">    

    <uta:field uta:id='tLogVoidFlag'
               uta:type='byte'
               uta:comment='Void flag' />

    <uta:field uta:id='tLogTipFlag' 	uta:type='byte'    uta:comment='Tip indicator' />

    <uta:field uta:id='tLogMTI'      uta:type='ASCII'      uta:length='lenMti'        uta:comment='Message Type' />
               
    <uta:field uta:id='tLogMnuItm'
               uta:type='word'
               uta:comment='menu item selected' />
    <uta:field uta:id='tLogDatTim'
               uta:type='ASCII'
               uta:length='lenDatTim'
               uta:comment='transaction date and time CCYYMMDDhhmmss' />
    <uta:field uta:id='tLogBinIdx' 
               uta:type='byte' 
               uta:comment='index in Bin multirecord' />
    <uta:field uta:id='tLogAcqIdx' 
               uta:type='byte' 
               uta:comment='index in Acq multirecord' />
    <uta:field uta:id='tLogIssIdx' 
               uta:type='byte' 
               uta:comment='index in Iss multirecord' />
    <uta:field uta:id='tLogPan'
               uta:type='ASCII'
               uta:length='lenPan'
               uta:comment='PAN' />
    <uta:field uta:id='tLogExpDat' 
               uta:type='ASCII'
               uta:length='lenExpDat'
               uta:comment='Expiry Date' />

    <uta:field uta:id='tLogExpDatFmt'        uta:type='ASCII'      uta:length='lenExpDat'         uta:comment='Expiry Date' />
               
    <uta:field uta:id='tLogTrk2'
               uta:type='ASCII'
               uta:length='lenTrk2'
               uta:comment='track 2 of the card' />
    <uta:field uta:id='tLogAutCod'
               uta:type='ASCII'
               uta:length='lenAutCod'
               uta:comment='Authorization code' />

    <uta:field uta:id='tLogAutCodOff' uta:type='ASCII' uta:length='lenAutCod' uta:comment='Authorization (approval) code' />
               
    <uta:field uta:id='tLogBatNum' 
               uta:type='ASCII'
               uta:length='lenBatNum'
               uta:comment='Batch Number' />
    <uta:field uta:id='tLogTraAmt' 
               uta:type='ASCII'
               uta:length='lenAmt'
               uta:comment='Transaction Amount' />

    <uta:field uta:id='tLogFmtAmt'    uta:type='ASCII'   uta:length='20' uta:comment='Display formatted Amount'/>
               
    <uta:field uta:id='tLogTipAmt' 
               uta:type='ASCII'
               uta:length='lenAmt'
               uta:comment='Tip Amount' />

    <uta:field uta:id='tLogFmtTip'    uta:type='ASCII'   uta:length='20' uta:comment='Display formatted Tip'/>

    <uta:field uta:id='tLogEmvICC' uta:type='ASCII' uta:length='lenEmvICC' uta:comment='message of bit 55' />
               
    <uta:field uta:id='tLogSTAN'
               uta:type='card'
               uta:comment='system trace audit number' />
    <uta:field uta:id='tLogRrn'
               uta:type='ASCII'
               uta:length='lenRrn'
               uta:comment='Retrieval Reference Number' />

    <uta:field uta:id='tLogRspCod' uta:type='ASCII' uta:length='lenRspCod' uta:comment='Response code' />
    <uta:field uta:id='tLogPinBlk' uta:type='binary' uta:length='lenPinBlk' uta:comment='Pin block' />                              
    <uta:field uta:id='tLogROC'
               uta:type='card'
               uta:comment='ROC/Invoice' />
    <uta:field uta:id='tLogEntMod'
               uta:type='byte'
               uta:comment='POS Entry Mode' />
    <uta:field uta:id='tLogConCode'
               uta:type='byte'
               uta:comment='POS Condition Code' />
    <uta:field uta:id='tLogCardType'
               uta:type='ASCII'
               uta:length='lenCardTransType'
               uta:comment='Card Type' />  
    
    <uta:field uta:id='tLogTraTrk1' uta:type='ASCII' uta:length='lenTrk2' uta:comment='track 1 of the card' />  <!-- @agmr - BRI -->

    <uta:field uta:id='tLogNomorRekeningAsalBRI'   uta:comment='Nomor rekening asal BRI = 15' uta:type='ASCII' uta:length='lenNomorRekeningBRI' />
    <uta:field uta:id='tLogNamaPenerima'           uta:comment='Nama penerima/rek. tujuan'  uta:type='ASCII' uta:length='lenNamaPemilik' />
    <uta:field uta:id='tLogSisaSaldo'              uta:comment='Sisa saldo'              uta:type='ASCII' uta:length='lenSisaSaldo' />
    <uta:field uta:id='tLogFee'                    uta:comment='Fee'                     uta:type='ASCII' uta:length='lenFee' />
  
</uta:record>